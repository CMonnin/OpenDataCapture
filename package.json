{
  "name": "ddcp",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "workspaces": [
    "api",
    "instruments",
    "marketing",
    "pwa",
    "types",
    "web"
  ],
  "scripts": {
    "build": "env-cmd cross-env NODE_ENV=production turbo run build",
    "check-types": "turbo run check-types",
    "clean": "fd -H -I --prune \".turbo|dist|node_modules\" --exec rm -rf",
    "dev": "env-cmd cross-env NODE_ENV=development turbo run dev",
    "format": "turbo run format",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "storybook": "npm run storybook --workspace web",
    "test": "env-cmd turbo run test"
  },
  "dependencies": {
    "@douglasneuroinformatics/ajv": "^0.0.4",
    "@douglasneuroinformatics/utils": "0.4.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@ddcp/types": "*",
    "@douglasneuroinformatics/eslint-config": "^0.1.7",
    "@douglasneuroinformatics/form-types": "^0.0.2",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "c8": "^8.0.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.46.0",
    "eslint-plugin-import": "^2.28.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6"
  },
  "packageManager": "npm@9.6.7",
  "engines": {
    "node": ">=20.0.0 <21.0.0",
    "npm": ">=9.0.0 <10.0.0"
  }
}
