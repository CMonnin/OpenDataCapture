{
  "name": "douglas-data-capture-platform",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "eslint": "8.28.0",
    "eslint-config-dnp": "*",
    "prettier": "2.8.0",
    "turbo": "1.6.3",
    "typescript": "4.9.3",
    "husky": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": ">=9.0.0 <10.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
