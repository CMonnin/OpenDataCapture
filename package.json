{
  "name": "open-data-capture",
  "version": "1.0.0-beta.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production env-cmd turbo run build --cache-dir=.turbo",
    "clean": "find . -type d '(' -name '.turbo' -o -name 'dist' -o -name 'node_modules' ')' -prune -exec rm -rf {} \\;",
    "clean:workflows": "./scripts/clean-workflows.sh",
    "cy:install": "cypress install",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "dev": "pnpm dev:core",
    "dev:all": "NODE_ENV=development env-cmd turbo run dev",
    "dev:core": "NODE_ENV=development env-cmd pnpm exec turbo run dev --filter=@open-data-capture/api --filter=@open-data-capture/gateway --filter=@open-data-capture/web",
    "force-reinstall": "pnpm clean && pnpm install",
    "format": "turbo run format",
    "generate:env": "./scripts/generate-env.sh",
    "lint": "turbo run lint --cache-dir=.turbo",
    "prepare": "husky install",
    "start": "NODE_ENV=production env-cmd turbo run start",
    "tag:create": "git tag -a v$(cat package.json | jq -r .version)",
    "tag:push": "git push upstream v$(cat package.json | jq -r .version)",
    "test": "env-cmd turbo run test --cache-dir=.turbo",
    "ws": "./scripts/workspace.js $@"
  },
  "devDependencies": {
    "@douglasneuroinformatics/eslint-config": "^3.0.3",
    "@douglasneuroinformatics/form-types": "^5.2.0",
    "@eslint/js": "^8.57.0",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-themes": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@swc-node/register": "^1.9.0",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.4",
    "@swc/helpers": "^0.5.6",
    "@turbo/workspaces": "^1.12.4",
    "@types/github-script": "github:actions/github-script",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/browser": "^1.3.1",
    "@vitest/coverage-v8": "^1.3.1",
    "astro-eslint-parser": "^0.16.3",
    "cypress": "^13.6.6",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-perfectionist": "^2.6.0",
    "eslint-plugin-react": "^7.34.0",
    "globals": "^13.24.0",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.12.3",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "start-server-and-test": "^2.0.3",
    "storybook": "^7.6.17",
    "storybook-react-i18next": "^2.0.10",
    "tsx": "^4.7.1",
    "turbo": "^1.12.4",
    "type-fest": "^4.10.3",
    "typescript": "~5.3.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.3.1",
    "webdriverio": "^8.32.4"
  },
  "packageManager": "pnpm@8.15.3"
}
