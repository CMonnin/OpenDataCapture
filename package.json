{
  "name": "open-data-capture",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "runtime/*"
  ],
  "scripts": {
    "build": "env-cmd cross-env NODE_ENV=production turbo run build --no-daemon",
    "build:api": "env-cmd cross-env NODE_ENV=production turbo run build --no-daemon --filter=@open-data-capture/api",
    "build:web": "env-cmd cross-env NODE_ENV=production turbo run build --no-daemon --filter=@open-data-capture/web",
    "clean": "find . -type d '(' -name '.turbo' -o -name 'dist' -o -name 'node_modules' ')' -prune -exec rm -rf {} \\;",
    "clean-workflows": "./scripts/clean-workflows.sh",
    "compose:dev": "docker compose -f docker-compose.dev.yaml",
    "cy:install": "cypress install",
    "db:generate": "turbo run db:generate --no-daemon",
    "db:push": "turbo run db:push --no-daemon",
    "dev": "bun dev:core",
    "dev:all": "env-cmd cross-env NODE_ENV=development turbo run dev --no-daemon",
    "dev:core": "env-cmd cross-env NODE_ENV=development bunx turbo run dev --no-daemon --filter=@open-data-capture/api --filter=@open-data-capture/gateway --filter=@open-data-capture/web",
    "force-reinstall": "bun pm cache rm && bun clean && bun install",
    "format": "turbo run format --no-daemon",
    "generate:env": "./scripts/generate-env.sh",
    "lint": "turbo run lint --no-daemon",
    "prepare": "husky install",
    "start": "env-cmd cross-env NODE_ENV=production turbo run start --no-daemon",
    "test": "env-cmd turbo run test --no-daemon",
    "ws": "./scripts/workspace.js $@"
  },
  "devDependencies": {
    "@douglasneuroinformatics/eslint-config": "latest",
    "@douglasneuroinformatics/form-types": "latest",
    "@eslint/js": "^8.56.0",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-themes": "^7.6.7",
    "@storybook/blocks": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@turbo/workspaces": "^1.11.3",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "astro-eslint-parser": "^0.16.1",
    "bun-types": "^1.0.22",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-react": "^7.33.2",
    "globals": "^13.24.0",
    "husky": "^8.0.3",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.3",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "start-server-and-test": "^2.0.3",
    "storybook": "^7.6.7",
    "storybook-react-i18next": "^2.0.10",
    "tsx": "^4.7.0",
    "turbo": "^1.11.3",
    "type-fest": "^4.9.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "bun@1.0.18"
}