{
  "name": "ddcp",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "workspaces": [
    "api",
    "instruments",
    "types",
    "web"
  ],
  "scripts": {
    "build": "env-cmd cross-env NODE_ENV=production turbo run build",
    "check-types": "turbo run check-types",
    "clean": "fd -H -I --prune \".turbo|dist|node_modules\" --exec rm -rf",
    "dev": "env-cmd cross-env NODE_ENV=development turbo run dev",
    "format": "turbo run format",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "storybook": "yarn ws client storybook",
    "test": "env-cmd turbo run test",
    "ws": "bash -c 'yarn workspace \"@ddcp/$0\" \"${@:1}\"'"
  },
  "dependencies": {
    "@douglasneuroinformatics/ajv": "^0.0.4",
    "@douglasneuroinformatics/utils": "0.2.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@ddcp/types": "*",
    "@douglasneuroinformatics/eslint-config": "^0.1.3",
    "@douglasneuroinformatics/form-types": "^0.0.2",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.59.6",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.44.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "turbo": "^1.10.7",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": ">=9.0.0 <10.0.0"
  }
}
