{
  "name": "open-data-capture",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "env-cmd cross-env NODE_ENV=production turbo run build",
    "check-types": "turbo run check-types",
    "clean": "fd -H -I --prune \".turbo|dist|node_modules\" --exec rm -rf",
    "dev": "env-cmd cross-env NODE_ENV=development turbo run dev",
    "format": "turbo run format",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "test": "env-cmd turbo run test",
    "ws": "./scripts/workspace.sh $@"
  },
  "dependencies": {
    "@douglasneuroinformatics/ajv": "^1.0.0",
    "@douglasneuroinformatics/utils": "^1.0.0",
    "ts-pattern": "^5.0.5"
  },
  "devDependencies": {
    "@douglasneuroinformatics/eslint-config": "^2.0.0",
    "@douglasneuroinformatics/form-types": "^1.0.0",
    "@open-data-capture/types": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "bun-types": "^1.0.3",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.50.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "turbo": "^1.10.14",
    "typescript": "^5.2.2"
  }
}