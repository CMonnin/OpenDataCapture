    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "prisma": "3.10.0",
    "tailwindcss": "^3.3.5"
  },
  "trustedDependencies": [
    "sqlite3"
  ]
