{
  "name": "@open-data-capture/web",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "build": "tsc && cross-env NODE_ENV=production bunx vite build",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "env-cmd -f ../../.env cross-env NODE_ENV=development bunx --bun vite",
    "format": "prettier --write src",
    "format:translations": "find src/translations -name '*.json' -exec bunx sort-json {} \\;",
    "lint": "tsc && eslint --fix src",
    "storybook": "env-cmd -f ../../.env storybook dev -p 6006",
    "test": "env-cmd -f ../../.env --use-shell 'start-server-and-test \"bunx vite --mode test\" \"http://localhost:${WEB_DEV_SERVER_PORT}\" cy:run'",
    "test:dev": "env-cmd -f ../../.env --use-shell 'start-server-and-test \"bunx vite --mode test\" \"http://localhost:${WEB_DEV_SERVER_PORT}\" cy:open'"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@douglasneuroinformatics/ui": "latest",
    "@douglasneuroinformatics/utils": "latest",
    "@headlessui/react": "^1.7.18",
