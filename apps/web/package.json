{
  "name": "@open-data-capture/web",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "build": "tsc && NODE_ENV=production NODE_OPTIONS='--import=tsx' pnpm exec vite build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "NODE_ENV=development env-cmd -f ../../.env pnpm exec vite",
    "format": "prettier --write src",
    "format:translations": "find src/translations -name '*.json' -exec pnpm exec sort-json {} \\;",
    "inject": "pnpm exec import-meta-env -x .env.public -p dist/index.html",
    "lint": "tsc && eslint --fix src",
    "start": "vite preview --port $WEB_DEV_SERVER_PORT",
    "storybook": "env-cmd -f ../../.env storybook dev -p 6006",
    "test": "env-cmd -f ../../.env --use-shell 'start-server-and-test \"pnpm exec vite --mode test\" \"http://localhost:${WEB_DEV_SERVER_PORT}\" cy:run'",
    "test:dev": "env-cmd -f ../../.env --use-shell 'start-server-and-test \"pnpm exec vite --mode test\" \"http://localhost:${WEB_DEV_SERVER_PORT}\" cy:open'"
  },
  "dependencies": {
    "@casl/ability": "^6.7.0",
    "@douglasneuroinformatics/ui": "^6.2.1",
    "@douglasneuroinformatics/utils": "^1.2.2",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@import-meta-env/cli": "^0.6.8",
    "@import-meta-env/unplugin": "^0.5.1",
    "@open-data-capture/common": "workspace:*",
    "@open-data-capture/demo": "workspace:*",
    "@open-data-capture/editor": "workspace:*",
    "@open-data-capture/i18next": "workspace:*",
    "@open-data-capture/instrument-interpreter": "workspace:*",
    "@open-data-capture/instrument-renderer": "workspace:*",
    "@open-data-capture/instrument-utils": "workspace:*",
    "@open-data-capture/react-core": "workspace:*",
    "@tanstack/react-query": "^5.26.3",
    "@tanstack/react-query-devtools": "^5.26.3",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "framer-motion": "^10.18.0",
    "i18next": "^23.10.1",
    "i18next-http-backend": "^2.5.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.2",
    "tailwind-merge": "^2.2.1",
    "ts-pattern": "^5.0.8",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@open-data-capture/tailwindcss": "workspace:*",
    "@open-data-capture/vite-plugin-plausible": "workspace:*",
    "@open-data-capture/vite-plugin-runtime": "workspace:*",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.18",
    "jose": "^5.2.3",
    "jsdom": "^23.2.0",
    "msw": "^2.2.3",
    "postcss": "^8.4.35",
    "prop-types": "^15.8.1",
    "rollup-plugin-copy": "^3.5.0",
    "sort-json": "^2.0.1",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.6",
    "vite-plugin-compression": "^0.5.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
