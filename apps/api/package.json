{
  "name": "@open-data-capture/api",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "build:watch": "node scripts/build.js --watch",
    "dev": "NODE_ENV=development env-cmd -f ../../.env concurrently \"pnpm build:watch\" \"sleep 1 && nodemon dist/app.mjs\"",
    "format": "prettier --write src",
    "lint": "tsc && eslint --fix src",
    "start": "env-cmd -f ../../.env --use-shell 'NODE_OPTIONS=\"--enable-source-maps\" API_PROD_SERVER_PORT=$API_DEV_SERVER_PORT NODE_ENV=production ESBUILD_BINARY_PATH=dist/bin/esbuild GATEWAY_SITE_ADDRESS=http://localhost:$GATEWAY_DEV_SERVER_PORT  node dist/app.mjs'",
    "test": "env-cmd -f ../../.env vitest run"
  },
  "dependencies": {
    "@casl/ability": "^6.7.0",
    "@casl/prisma": "^1.4.1",
    "@douglasneuroinformatics/libjs": "^0.0.1",
    "@douglasneuroinformatics/nestjs": "^3.0.0",
    "@faker-js/faker": "^8.4.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "2.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/throttler": "^5.1.2",
    "@open-data-capture/common": "workspace:*",
    "@open-data-capture/crypto": "workspace:*",
    "@open-data-capture/database": "workspace:*",
    "@open-data-capture/demo": "workspace:*",
    "@open-data-capture/instrument-interpreter": "workspace:*",
    "@open-data-capture/instrument-library": "workspace:*",
    "@open-data-capture/instrument-transformer": "workspace:*",
    "@open-data-capture/runtime-v0.0.1": "workspace:*",
    "@open-data-capture/stats": "workspace:*",
    "axios": "^1.6.7",
    "express": "^4.18.3",
    "lodash": "^4.17.21",
    "mongodb": "^6.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "ts-pattern": "^5.0.8",
    "unidecode": "^0.1.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@douglasneuroinformatics/esbuild-plugin-native-modules": "^0.0.1",
    "@douglasneuroinformatics/esbuild-plugin-prisma": "^0.0.1",
    "@nestjs/testing": "^10.3.3",
    "@open-data-capture/esbuild-plugin-runtime": "workspace:*",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/unidecode": "^0.1.3",
    "concurrently": "^8.2.2",
    "esbuild": "^0.20.1",
    "esbuild-plugin-tsc": "^0.4.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4"
  }
}
