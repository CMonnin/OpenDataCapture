{
  "name": "@open-data-capture/api",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "build": "npm run check-types && swc --copy-files --delete-dir-on-start --out-dir dist src",
    "check-types": "tsc",
    "dev": "NODE_ENV=development concurrently \"swc --copy-files --out-dir dist -w src\" \"nodemon -L dist/main.js\"",
    "format": "prettier --write src",
    "lint": "eslint --fix src",
    "start": "node ./dist/main.js",
    "test": "node --loader @swc-node/register/esm --no-warnings --test $(find src -type f -name \"*.spec.ts\")",
    "test:coverage": "c8 --all --src=src -r json -r 'text' -x '**/*.{d,dto,entity,module,spec}.ts' -x 'src/docs.ts' -x 'src/main.ts' npm test",
    "test:e2e": "NODE_ENV=testing node --loader @swc-node/register/esm --no-warnings --test $(find test -type f -name \"*.spec.ts\")"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,cjs,mjs,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@casl/mongoose": "^7.2.1",
    "@douglasneuroinformatics/stats": "^2.0.0",
    "@faker-js/faker": "^8.1.0",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.6",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "2.0.2",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.6",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/throttler": "^5.0.0",
    "@open-data-capture/demo": "workspace:*",
    "@open-data-capture/instruments": "workspace:*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mongoose": "^7.5.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "unidecode": "^0.1.8"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.6",
    "@types/express": "^4.17.18",
    "@types/passport": "^1.0.13",
    "@types/passport-jwt": "^3.0.10",
    "@types/supertest": "^2.0.13",
    "@types/unidecode": "^0.1.1",
    "supertest": "^6.3.3"
  }
}