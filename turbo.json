{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".env"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^build"
      ],
      "persistent": true
    },
    "format": {},
    "inject": {
      "dependsOn": [
        "build"
      ]
    },
    "lint": {},
    "start": {
      "dependsOn": [
        "build",
        "inject"
      ],
      "persistent": true
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
