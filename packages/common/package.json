{
  "name": "@douglasneuroinformatics/common",
  "version": "0.0.2",
  "license": "AGPL-3.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/mjs/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js"
    },
    "./auth": {
      "require": "./dist/cjs/auth/index.js",
      "import": "./dist/mjs/auth/index.js"
    },
    "./core": {
      "require": "./dist/cjs/core/index.js",
      "import": "./dist/mjs/core/index.js"
    },
    "./groups": {
      "require": "./dist/cjs/groups/index.js",
      "import": "./dist/mjs/groups/index.js"
    },
    "./instruments": {
      "require": "./dist/cjs/instruments/index.js",
      "import": "./dist/mjs/instruments/index.js"
    },
    "./subjects": {
      "require": "./dist/cjs/subjects/index.js",
      "import": "./dist/mjs/subjects/index.js"
    },
    "./users": {
      "require": "./dist/cjs/users/index.js",
      "import": "./dist/mjs/users/index.js"
    },
    "./utils": {
      "require": "./dist/cjs/utils/index.js",
      "import": "./dist/mjs/utils/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && yarn build:cjs && yarn build:mjs",
    "build:cjs": "tsc --outDir dist/cjs -m nodenext",
    "build:mjs": "tsc --outDir dist/mjs --m esnext",
    "build:types": "tsc --outDir dist/types --declaration --emitDeclarationOnly --declarationMap",
    "check-types": "tsc --noEmit",
    "lint": "eslint --fix src"
  }
}
