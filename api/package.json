{
  "name": "@ddcp/api",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "build": "swc --copy-files --delete-dir-on-start --out-dir dist src",
    "check-types": "tsc",
    "dev": "NODE_ENV=development concurrently \"swc --copy-files --out-dir dist -w src\" \"nodemon -L dist/main.js\"",
    "format": "prettier --write .",
    "lint": "eslint --fix src",
    "start": "node ./dist/main.js",
    "test": "find src -type f -name \"*.spec.ts\" | node --loader @swc-node/register/esm --no-warnings --test"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@casl/mongoose": "^7.1.3",
    "@ddcp/instruments": "*",
    "@douglasneuroinformatics/node-ext": "^0.0.3",
    "@douglasneuroinformatics/stats": "^0.0.2",
    "@faker-js/faker": "^8.0.2",
    "@nestjs/common": "^10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "2.0.2",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/swagger": "^7.0.4",
    "@nestjs/throttler": "^4.1.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mongoose": "^7.3.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "unidecode": "^0.1.8"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.0.3",
    "@swc-node/register": "^1.6.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.66",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@types/unidecode": "^0.1.1",
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}
